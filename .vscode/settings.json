{
  "files.exclude": {
    "**/bin": true,
    "**/obj": true,
    "**/node_modules": true,
    "**/.angular": true,
    "**/*.Tests": true,
  },
  "cSpell.language": "en",
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
  ],
  "cSpell.words": [
    "ASPNETCORE",
    "dotnet",
    "globaljson",
    "mstest",
    "Npgsql",
    "srms",
    "SRMS",
    "webapi"
  ],
  "conventionalCommits.scopes": [
    "backend",
    "frontend",
    "testings",
  ],
}