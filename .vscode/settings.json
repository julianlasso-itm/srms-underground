{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.bundle": true,
    "**/bin": true,
    "**/obj": true,
    "**/node_modules": true,
    "**/.angular": true,
    "**/*.Tests": true,
    "**/*.unotation": true
  },
  "cSpell.language": "en",
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "package.json",
  ],
  "cSpell.words": [
    "ASPNETCORE",
    "dotnet",
    "globaljson",
    "hmac",
    "HMACSHA",
    "mstest",
    "Npgsql",
    "orderszulu",
    "srms",
    "SRMS",
    "webapi"
  ],
  "conventionalCommits.scopes": [
    "backend",
    "frontend",
    "testings",
  ],
  "material-icon-theme.folders.associations": {
    "Infrastructure": "cluster",
    "Application": "app",
    "Domain": "core",
    "Properties": "config",
    "Persistence": "database",
    "Migrations": "update",
    "ApiGateway": "connection",
    "AccessControl": "guard",
    "Profiles": "layout",
    "Assessments": "messages",
    "QueryBank": "rules",
    "Analytics": "robot",
    "ProtocolBuffers": "proto",
    "EventConsumer": "queue",
    "DataAnnotations": "middleware",
  },
  "CodeGPT.apiKey": "CodeGPT Plus Beta",
  "CodeGPT.Autocomplete.provider": "CodeGPT Plus - Plus",
  "CodeGPT.Autocomplete.maxTokens": 1250,
  "CodeGPT.Autocomplete.enabled": false,
  "bigUML.hideUnotation": true,
  "CodeGPT.Autocomplete.suggestionDelay": 0
}